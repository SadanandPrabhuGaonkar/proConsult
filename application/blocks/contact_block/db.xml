<?xml version="1.0"?>
<schema version="0.3">
    <table name="btContactBlock">
        <field name="bID" type="I">
            <key/>
            <unsigned/>
        </field>
        <field name="title" type="C" size="255"></field>
        <field name="subtitle" type="C" size="255"></field>
        <field name="number" type="C" size="255"></field>
        <field name="email" type="C" size="255"></field>
        <field name="linkedin" type="C" size="255"></field>
    </table>
    <table name="btContactBlockStackEntries">
        <field name="id" type="I">
            <key/>
            <unsigned/>
            <autoincrement/>
        </field>
        <field name="bID" type="I">
            <unsigned/>
        </field>
        <field name="stID" type="I">
            <unsigned/>
        </field>
        <field name="sortOrder" type="I"></field>
    </table>
</schema>